#!/bin/bash
source ScriptFunctions

# the sed regex first removes comments and empty lines, then find the NAME property and returns the value
for interfacename in $(sed -r '/^[\ \t]*#.*/d;/^[\ \t]*$/d;s,.*NAME="([^"]*)".*,\1,g' "${goboSettings}"/udev/rules.d/70-persistent-net.rules)
do
   ifconfig ${interfacename} &> /dev/null && echo ${interfacename}
done
